if (isDraw(gameState.board)) {
      gameState.gameOver = true;
      return message.reply(`ğ»ğ‘œÌ€ğ‘ ğ‘Ÿğ‘œÌ‚Ì€ğ‘–! ğŸ¤\n${printBoard(gameState.board)}`);
    }

    const computerPos = computerMove(gameState.board);
    if (isWin(gameState.board, 'O')) {
      gameState.gameOver = true;
      return message.reply(`ğµğ‘Ì£ğ‘› Ä‘ğ‘Ìƒ ğ‘¡â„ğ‘¢ğ‘âš°ï¸â˜ ï¸! ğŸ˜¢ ğ— ğ—®Ìğ˜† Ä‘ğ—®Ìƒ ğ—°ğ—µğ—¼Ì£ğ—» ğ—¼Ì‚ ${computerPos}.\n${printBoard(gameState.board)}`);
    }

    if (isDraw(gameState.board)) {
      gameState.gameOver = true;
      return message.reply(`ğ»ğ‘œÌ€ğ‘ ğ‘Ÿğ‘œÌ‚Ì€ğ‘–! ğŸ¤ğ— ğ—®Ìğ˜† Ä‘ğ—®Ìƒ ğ—°ğ—µğ—¼Ì£ğ—» ğ—¼Ì‚ ${computerPos}.\n${printBoard(gameState.board)}`);
    }

    return message.reply(`Báº¡n Ä‘Ã£ chá»n Ã´ ${playerMove}.\n${printBoard(gameState.board)}\nğ‘ºğ’Ì›ğ’ ğ’‘ğ’†Ìğ’ Ä‘ğ’‚ğ’ğ’ˆ ğ’”ğ’–ğ’š ğ’ğ’ˆğ’‰ğ’ŠÌƒâŒšğŸ¥±... ğ‘ºğ’Ì›ğ’ ğ’‘ğ’†Ìğ’ Ä‘ğ˜¢Ìƒ ğ˜¤ğ˜©ğ˜°Ì£ğ˜¯ ğ˜°Ì‚ ${computerPos}.`);
  }
};
